html 
    head 
        title WEB - #{_title}
        meta(charset="utf-8")
    body 
        h1 
            a(href="/") WEB 
        h1
            ul
                each val in _list
                    li 
                        a(href="/page/#{val}") #{val}
            a(href="/create") create
            |  
            a(href="/update/#{_title}") update
        form(action="delete" method="post")
            input(type="hidden" name="id" value="#{_title}")
            p 
            input(type="submit" value="delete")

        h2=_title
        p=_description